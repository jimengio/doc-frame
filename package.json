{
  "name": "@jimengio/doc-frame",
  "version": "0.1.2",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/",
    "prepare": "yarn compile",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/doc-frame",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/router-code-generator": "^0.1.5",
    "@jimengio/ruled-router": "^0.2.14",
    "@jimengio/shared-utils": "^0.1.3",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.1.0",
    "dayjs": "^1.8.15",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "emotion": "^10.0.14",
    "file-loader": "^4.1.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^1.4.3",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.13",
    "http-server": "^0.11.0",
    "immer": "^3.1.3",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "typescript-styled-plugin": "^0.14.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-hud": "^0.1.2"
  },
